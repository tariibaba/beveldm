{
  "name": "beveldm",
  "productName": "Bevel Download Manager",
  "author": "Ayibatari Ibaba <tariibabadev@gmail.com>",
  "version": "0.4.0",
  "private": true,
  "main": "public/electron.js",
  "description": "Cross-platform download manager",
  "homepage": ".",
  "build": {
    "appId": "com.tariibaba.beveldm",
    "productName": "Bevel Download Manager",
    "artifactName": "BevelDM-Installer-${version}-${os}-${arch}.${ext}",
    "icon": "images/app-icon.png",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32",
            "x64"
          ]
        }
      ]
    },
    "mac": {
      "target": [
        "zip"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "tar.gz"
      ]
    },
    "protocols": [
      {
        "name": "beveldm",
        "role": "Viewer",
        "schemes": [
          "beveldm"
        ]
      }
    ]
  },
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "await-timeout": "^1.0.1",
    "clsx": "^1.0.4",
    "compare-urls": "^2.0.0",
    "content-disposition-filename": "^0.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-store": "^5.0.0",
    "filename-reserved-regex": "^2.0.0",
    "hex-to-rgba": "^2.0.1",
    "humanize-duration": "^3.23.1",
    "is-online": "^8.2.1",
    "lodash.groupby": "^4.6.0",
    "moment": "^2.29.2",
    "node-notifier": "^8.0.1",
    "open": "^7.0.1",
    "path-exists": "^4.0.0",
    "pify": "^4.0.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-scripts": "3.0.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "remote-file-info": "^1.0.1",
    "sum-by": "^1.0.3",
    "tmp": "^0.2.1",
    "typeface-roboto": "^0.0.75",
    "unique-filename": "^1.1.1",
    "uuid": "^3.3.2",
    "valid-url": "^1.0.9",
    "when-expression": "^0.0.2",
    "windows-system-icon": "^0.0.8",
    "youtube-url": "^0.5.0",
    "ytdl-core": "^4.4.4"
  },
  "scripts": {
    "rstart": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "preepack": "yarn build",
    "epack": "electron-builder --dir",
    "predist": "yarn build",
    "dist": "node scripts/build.js",
    "start": "electron --remote-debugging-port=9223 .",
    "dev": "nodemon --exec \"yarn start\"",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.8.3",
    "@hot-loader/react-dom": "^17.0.1",
    "@types/jest": "24.9.0",
    "@types/node-notifier": "^6.0.1",
    "electron": "^18.3.7",
    "electron-builder": "^22.9.1",
    "file-loader": "^5.0.2",
    "nodemon": "^2.0.15",
    "react-app-rewire-hot-loader": "^2.0.1",
    "react-app-rewired": "^2.1.8",
    "react-hot-loader": "^4.13.0"
  }
}
